#!/bin/sh

# dist-test-cases --- listing files and directories under Units and Tmain
#
# COPYRIGHT NOTICE SHOULD BE HERE
#

echo "# -*- makefile -*-"
echo "# Generated by $0" &&
echo "EXTRA_DIST += misc/units \\" &&
find Tmain Units -type f -exec printf "    %s\\\\\n" '{}' \; &&
echo '$(NULL)' &&

exit $?
